<UserControl x:Class="Keyboard.HeatMap.Controls.TimeLine"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Keyboard.HeatMap.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        
        <Button Grid.Column="0" FontWeight="Bold" ToolTip="11">
            <TextBlock Text="&#xE26C;" FontFamily="Segoe MDL2 Assets" FontSize="20" />
        </Button>
        <StackPanel Grid.Column="1">
        </StackPanel>
        <Button Grid.Column="2">
            <TextBlock Text="&#xE26B;" FontFamily="Segoe MDL2 Assets" FontSize="20"/>
        </Button>
    </Grid>
</UserControl>
